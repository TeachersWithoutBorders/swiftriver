          :locality => json["geonames"][0]["name"],
          :administrative_area => json["geonames"][0]["adminName1"],
          :country_code => "IN",
          :geo_source_id => json["geonames"][0]["geonameId"]} 
          puts "#{c} @ [#{location_attr[:lat]}, #{location_attr[:lon]}]"
          location_attr
        end
      rescue
        File.open("locations.yml","w") {|f| f << locations.to_yaml }
      end

      File.open("locations.yml","w") {|f| f << locations.to_yaml }
